local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Wheat Hubs More Games", HidePremium = false, SaveConfig = true, ConfigFolder = "UI_Wheat_Hub"})

local HttpService = game:GetService("HttpService")

local lower = string.lower;
request = http_request or request or HttpPost or syn.request
local function join_server(code)
    spawn(function()
        pcall(function()
            return request({
                Url = "http://127.0.0.1:6463/rpc?v=1";
                Method = "POST";
                Body = HttpService:JSONEncode({
                    cmd = "INVITE_BROWSER",
                    args = {
                        code = code
                    },
                    nonce = lower(HttpService:GenerateGUID(false))
                });
                Headers = {
                    ["Content-Type"] = "application/json",
                    ["Origin"] = "https://discord.com"
                }
            });
        end)
    end)
end;

join_server('wsbyxxCQuW')

local Credits_Tab = Window:MakeTab({
	Name = "Credits",
	Icon = "",
	PremiumOnly = false
})

Credits_Tab:AddButton({
	Name = "MaGiXx#6964",
	Callback = function()
      	setclipboard("MaGiXx#6964")
        join_server('wsbyxxCQuW')
  	end    
})

Credits_Tab:AddButton({
	Name = "Wheat Hub Discord",
	Callback = function()
      	setclipboard("https://discord.gg/wsbyxxCQuW")
        join_server('wsbyxxCQuW')
  	end    
})

